#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  --no-deprecation "$basedir/../../../../node_modules/gettext.js/bin/po2json" "$@"
  ret=$?
else 
  node  --no-deprecation "$basedir/../../../../node_modules/gettext.js/bin/po2json" "$@"
  ret=$?
fi
exit $ret
